{% extends 'baseback.html.twig' %}

{% block title %}Categorie{% endblock %}

{% block content %}
    <h1>Categorie</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ categorie.id }}</td>
            </tr>
            <tr>
                <th>Nom_Cat</th>
                <td>{{ categorie.NomCat }}</td>
            </tr>
            <tr>
                <th>Type_Cat</th>
                <td>{{ categorie.TypeCat }}</td>
            </tr>
        </tbody>
    </table>
    <a href="{{ path('categorie_edit', {'id': categorie.id}) }}"><button type="button" class="btn btn-outline-success">Modifier</button></a>




    <a href="{{ path('categorie_indexback') }}"><button type="button" class="btn btn-outline-warning">Retour</button></a>
    <a>{{ include('categorie/_delete_form.html.twig') }}</a>


{% endblock %}
